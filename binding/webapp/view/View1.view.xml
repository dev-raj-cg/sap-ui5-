<mvc:View controllerName="binding.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
           	<List id = "list"
		headerText="Products"
		items="{
			path: 'myData>/Products'
		}" >
		<StandardListItem id = "stnList"
			title="{myData>ProductID}"
			counter="{myData>ProductName}"/>
	</List>
        </content>
    </Page>
</mvc:View>
