<mvc:View controllerName="vaccination.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
    <Page id="page">
        <content>
        	
			<Toolbar>
				<Button type="Back" press="onPress" />
				<ToolbarSpacer/>
				<Title text="Vaccination Tracker in SAP" level="H2"/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://edit" type="Transparent" press="onPress" ariaLabelledBy="editButtonLabel"/>
			</Toolbar>

		
		
			<Toolbar>
				<ToolbarSpacer/>
					<RadioButton
						valueState = "Success"
						text="View in Table">
					</RadioButton>
					<RadioButton
						valueState = "Error"
						text="View in Calendar">
					</RadioButton>
				<ToolbarSpacer/>
			</Toolbar>
<Table id="idProductsTable"
		inset="false"
		items="{vacc>/}">

		<infoToolbar>
			<OverflowToolbar>
				<Label text="Vaccination Dashboard"/>
			</OverflowToolbar>
		</infoToolbar>
		<columns>
			<Column
				width="12em">
				<Text text="Vaccination Name" />
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true">
				<Text text="Company" />
			</Column>
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="End">
				<Text text="Date of Appointment" />
			</Column>
			<Column
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<Text text="Type of Vaccination" />
			</Column>

		</columns>
		<items>
			<ColumnListItem vAlign="Middle">
				<cells>
					<Text text = "{oVacc>vacc}"></Text>

					 />
				</cells>
			</ColumnListItem>
		</items>
	</Table>

		
        
        </content>
    </Page>
</mvc:View>
